<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
	xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">
	<persistence-unit name="crm" transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<!-- converter -->
		<class>aero.minova.crm.model.converter.IntegerListConverter</class>
		<class>aero.minova.crm.model.converter.TicketListConverter</class>
		<!-- model -->
		<class>aero.minova.crm.model.jpa.Attachment</class>
		<class>aero.minova.crm.model.jpa.MarkupText</class>
		<class>aero.minova.crm.model.jpa.Milestone</class>
		<class>aero.minova.crm.model.jpa.Ticket</class>
		<class>aero.minova.crm.model.jpa.TicketAttachment</class>
		<class>aero.minova.crm.model.jpa.TicketComponent</class>
		<class>aero.minova.crm.model.jpa.TicketCustomerPrio</class>
		<class>aero.minova.crm.model.jpa.TicketCustomerState</class>
		<class>aero.minova.crm.model.jpa.TicketCustomerType</class>
		<class>aero.minova.crm.model.jpa.TicketPriority</class>
		<class>aero.minova.crm.model.jpa.TicketProperty</class>
		<class>aero.minova.crm.model.jpa.TicketResolution</class>
		<class>aero.minova.crm.model.jpa.TicketState</class>
		<class>aero.minova.crm.model.jpa.TicketType</class>
		<class>aero.minova.crm.model.jpa.Todo</class>
		<class>aero.minova.crm.model.jpa.Wiki</class>
		<class>aero.minova.crm.model.jpa.WikiAttachment</class>
	</persistence-unit>
</persistence>